        android:layout_height="40dp"
        android:gravity="center"
        android:text="最低"
        android:textColor="#999999"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/right_tab_textview8"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="换手率"
        android:textColor="#999999"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/right_tab_textview9"
